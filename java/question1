import java.util.*;
public class as1 {
    public static void main(String[] args){
        int[] a={1,2,3,4};
        ArrayList<Integer> list=new ArrayList<Integer>();
        for(int i=0;i<a.length;i++){
            list.add(a[i]);
        }
        Collections.shuffle(list);
        for(int i=0;i<list.size();i++){
            a[i]=list.get(i);
        }
        for(int i=0;i<a.length;i++){
            System.out.print(a[i]+" ");
        }

    }
}
